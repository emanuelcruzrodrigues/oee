<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="br.feevale.tc.oee.domain.Equipment" table="equipment">

		<id name="id" column="id" />
		<timestamp name="dtLastUpdate" column="dt_last_update" />
		
		<property name="name" column="name" />
		<property name="dtCreation" column="dt_creation" />
		<property name="ipLastUpdate" column="ip_last_update" />
	</class>
</hibernate-mapping>