<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="br.feevale.tc.oee.domain.Equipment" table="equipment">

		<id name="id" column="id">
			<generator class="sequence">
				<param name="sequence">sq_equipment</param>
			</generator>
		</id>
		<timestamp name="dtUpdate" column="dt_update" />
		
		<property name="name" column="name" />
		<property name="dtCreation" column="dt_creation" />
	</class>
</hibernate-mapping>